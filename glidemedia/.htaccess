Options All -Indexes
<IfModule mod_php5.c>
php_flag engine 0
</IfModule>

<IfModule mod_rewrite.c>

############################################
## enable rewrites

    Options +FollowSymLinks
    RewriteEngine on

############################################
## rewrite everything else to index.php

    RewriteRule .* index.php [L]
</IfModule>
